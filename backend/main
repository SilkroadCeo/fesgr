        "currency": payment_data["currency"],
        "wallet": payment_data["wallet"],
        "status": "pending",  # Ğ˜Ğ·Ğ¼ĞµĞ½ĞµĞ½Ğ¾ Ğ½Ğ° pending Ğ´Ğ»Ñ Ğ¾Ğ¶Ğ¸Ğ´Ğ°Ğ½Ğ¸Ñ Ğ¿Ğ¾Ğ´Ñ‚Ğ²ĞµÑ€Ğ¶Ğ´ĞµĞ½Ğ¸Ñ
