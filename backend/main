    # ĞŸĞµÑ€ĞµĞ¼ĞµÑˆĞ¸Ğ²Ğ°ĞµĞ¼ Ğ´Ğ»Ñ Ñ€Ğ°Ğ½Ğ´Ğ¾Ğ¼Ğ½Ğ¾Ğ³Ğ¾ Ğ¾Ñ‚Ğ¾Ğ±Ñ€Ğ°Ğ¶ĞµĞ½Ğ¸Ñ
    import random
    random.shuffle(vip_profiles)

    return {"profiles": vip_profiles}

@app.get("/api/vip-catalogs")
async def get_vip_catalogs():
    """ĞŸĞ¾Ğ»ÑƒÑ‡Ğ¸Ñ‚ÑŒ Ğ½Ğ°ÑÑ‚Ñ€Ğ¾Ğ¹ĞºĞ¸ VIP ĞºĞ°Ñ‚Ğ°Ğ»Ğ¾Ğ³Ğ¾Ğ²"""
    data = load_data()
